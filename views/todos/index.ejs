<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>Express Lab</title>
</head>
<body>
    <h1>Artists</h1>
    <% if(error) { %>
        <h2><%= error %></h2>
    <%  } %>
    <ul>
        <% artist.forEach(function(person) { %>
            <li>
                <%= person.name %> 
                -
                <%= person.genre %> 
            </li>
        <% }) %> 
    </ul>
</body>
</html>